// <library>.ff

f(get) {

assign +++ 'list' >> ([dog, cat, jabali, ([])]) ;

}

// <main>.f

%CALL_LIBRARY '<library>.ff'

%CALL_LIBRARY 'io.ff'

f() {

io++list ['cat'] {

io++z ['YOU ANSWERED PROPERLY CONGRATS!'];

   };

io++list ['dog, jabaly'] {

io++z ['IM SORRY BUT TRY AGAIN!'];

  };

io++list ['([])'] {

io++z ['ERROR, PROGRAM WILL SHUT DOWN NOW'];

 };

};




